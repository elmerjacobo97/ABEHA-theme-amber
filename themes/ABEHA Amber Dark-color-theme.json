    "sideBar.background": "#141414",
    "sideBar.foreground": "#cccccc",
    "sideBarTitle.foreground": "#ff9e18",
    "sideBarSectionHeader.background": "#1a1a1a",
    "sideBarSectionHeader.foreground": "#ff9e18",

    "statusBar.background": "#1a1a1a",
    "statusBar.foreground": "#cccccc",
    "statusBar.noFolderBackground": "#1a1a1a",
    "statusBar.debuggingBackground": "#ff9e18",
    "statusBar.debuggingForeground": "#000000",
    "statusBar.border": "#2a2a2a",

    "titleBar.activeBackground": "#1a1a1a",
    "titleBar.activeForeground": "#cccccc",
    "titleBar.inactiveBackground": "#1a1a1a",
    "titleBar.inactiveForeground": "#666666",
    "titleBar.border": "#2a2a2a",

    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#ff9e18",
    "tab.inactiveBackground": "#0d0d0d",
    "tab.inactiveForeground": "#888888",
    "tab.border": "#0d0d0d",
    "tab.activeBorder": "#ff9e18",

    "button.background": "#ff9e18",
    "button.foreground": "#000000",
    "button.hoverBackground": "#ffb347",

    "input.background": "#1a1a1a",
    "input.border": "#2a2a2a",
    "input.foreground": "#e0e0e0",
    "inputOption.activeBorder": "#ff9e18",
    "focusBorder": "#ff9e18",

    "list.activeSelectionBackground": "#ff9e1833",
    "list.activeSelectionForeground": "#ff9e18",
    "list.hoverBackground": "#1f1f1f",
    "list.inactiveSelectionBackground": "#262626",
    "list.focusOutline": "#ff9e18",

    "panel.background": "#141414",
    "panel.border": "#2a2a2a",
    "panelTitle.activeForeground": "#ff9e18",
    "panelTitle.inactiveForeground": "#888888",

    "terminal.foreground": "#e0e0e0",
    "terminal.ansiYellow": "#ff9e18",
    "terminal.ansiBrightYellow": "#ffb347",

    "badge.background": "#ff9e18",
    "badge.foreground": "#000000",

    "progressBar.background": "#ff9e18",

    "notificationLink.foreground": "#ff9e18",

    "editorWidget.background": "#1a1a1a",
    "editorWidget.border": "#ff9e18",

    "peekView.border": "#ff9e18",
    "peekViewTitle.background": "#1a1a1a",
    "peekViewResult.background": "#1a1a1a",
    "peekViewEditor.background": "#141414"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6a6a6a"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff9e18",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#ffb347"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#ff8080"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#ff8080"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ffc266"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#a8d08d"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#ff9e18"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffb347"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ff9e18"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["text.html.basic entity.other.attribute-name.html", "text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#ff9e18"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#ffb347"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ff9e18"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
